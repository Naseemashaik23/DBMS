1.
  select * 
  from emp
2.
  select * 
  from emp 
  where DEPTNO = 10
3.
  select * 
  from emp 
  where DEPTNO <> 30
4.
  select * 
  from emp 
  ORDER BY SAL desc
5.
  select * 
  from emp 
  where rownum < 6
10.
  select *
  from emp
  where MGR IN(7698,7566) AND SAL > 1500;
11.
  SELECT *
  FROM emp
  where SAL < (SELECT SAL
   			      FROM emp
   			      WHERE ENAME = 'FORD' );

